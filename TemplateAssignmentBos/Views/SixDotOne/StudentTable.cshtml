@using Microsoft.EntityFrameworkCore.Metadata.Internal
<table class="table">
    <thead>
    <tr>
        <th>Id</th>
        <th>First Name</th>
        <th>Last Name</th>
        @if (ViewBag.AccessLevel >= 8)
        {
            <th>Grade</th>
        }
    </tr>
    </thead>
    <tbody>
    @foreach (var student in Model)
    {
        <tr>
            <td>@student.StudentId</td>
            <td>@student.FirstName</td>
            <td>@student.LastName</td>
            @if (ViewBag.AccessLevel >= 8)
            {
                <td>@student.Grade</td>
            }
        </tr>
    }
    </tbody>
</table>